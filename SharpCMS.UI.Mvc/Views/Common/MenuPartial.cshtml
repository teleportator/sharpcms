@using SharpCMS.BusinessLogic.Views
@using SharpCMS.UI.Mvc.Models.Common
@model MainMenuModel
@{
    Layout = null;
}

@Html.Raw("<ul class='menu'>")
@foreach (SiteMapItem siteNode in Model.SiteNodes)
{
	<li class="menu-item">
		<a class="menu-item-link" href="@siteNode.Url">@siteNode.Title</a>
	</li>
}
@Html.Raw("</ul>")